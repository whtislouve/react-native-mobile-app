{
  "name": "boiler",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean": "(cd android && ./gradlew clean)",
    "release": "(cd android && ./gradlew app:assembleRelease) && yarn renameAndroid",
    "all": "yarn install && (cd ios && pod install)",
    "podinstall": "(cd ios && pod install)",
    "reinstall": "rm -rf ./node_modules && yarn install  && (cd ios rm -rf ./Pods && pod install)",
    "tsw": "yarn run tsc --watch",
    "renameAndroid": "chmod u=rwx ./renameReleaseAndroidApk.bash && ./renameReleaseAndroidApk.bash",
    "fixWebView": "chmod 777 removeWebView.sh && sudo ./removeWebView.sh",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.9",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "@types/redux-logger": "^3.0.9",
    "axios": "^0.21.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-native": "^0.64.2",
    "react-native-bootsplash": "^3.2.0",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-localization": "^2.1.7",
    "react-native-orientation-locker": "^1.3.1",
    "react-native-reanimated": "1.13.3",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.1.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1",
    "typescript-fsa-redux-thunk": "^2.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.10.3",
    "@babel/runtime": "^7.12.5",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.171",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.64.2",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "redux-logger": "^3.0.6",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
